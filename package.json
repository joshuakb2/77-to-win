{
  "name": "77-to-win",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "make": "npm run make-elm && npm run make-ts && npm run make-worker && npm run unzip-maps",
    "make-debug": "npm run make-elm-debug && npm run make-ts && npm run make-worker && npm run unzip-maps",
    "make-worker": "cd src && rollup -c rollup.worker-config.js",
    "make-elm": "cd src/elm && elm make --output=../main.js Main.elm",
    "make-elm-debug": "cd src/elm && elm make --output=../main.js --debug Main.elm",
    "make-ts": "cd src && rollup -c rollup.config.js",
    "unzip-maps": "for z in *.json.zip src/maps/*.json.zip; do if ! [[ -f ${z%.zip} ]]; then unzip -d $(dirname $z) $z; fi; done",
    "elm": "elm",
    "tsc": "tsc"
  },
  "author": "Joshua Baker",
  "license": "ISC",
  "dependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/d3": "^5.16.3",
    "@types/d3-geo": "^2.0.0",
    "@types/geojson": "^7946.0.7",
    "@types/topojson-client": "^3.0.0",
    "d3-geo": "^2.0.1",
    "elm": "^0.19.1-3",
    "geojson": "^0.5.0",
    "rollup": "^2.28.2",
    "topojson-client": "^3.1.0",
    "tslib": "^2.0.2",
    "typescript": "^4.0.3"
  }
}
